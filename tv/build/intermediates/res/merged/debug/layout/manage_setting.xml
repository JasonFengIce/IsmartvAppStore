            android:layout_alignParentTop="true"
            android:layout_marginLeft="@dimen/px30"
            android:layout_marginTop="@dimen/px9"
            android:text="@string/setting_upgrade_top"
            android:textColor="@android:color/white"
            android:textSize="@dimen/px36" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="@dimen/px9"
            android:layout_marginLeft="@dimen/px30"
            android:text="@string/setting_upgrade_bottom"
            android:textColor="@android:color/white"
            android:textSize="@dimen/px30" />

        <TextView
            android:id="@+id/tv_upgrade_flag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/px30"
            android:textColor="@android:color/white"
            android:textSize="@dimen/px36" />
    </reco.frame.tv.view.TvRelativeLayout>

    <reco.frame.tv.view.TvRelativeLayout
        android:id="@+id/trl_about"
        android:layout_width="@dimen/px1112"
        android:layout_height="@dimen/px100"
        android:layout_below="@+id/trl_upgrade"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/px10"
        android:background="@drawable/shape_rectangle_trusculent"
        tv:cursorRes="@drawable/shape_rectangle_cursor"
        tv:boarder="@dimen/px2"
        tv:scalable="false" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/px30"
            android:text="@string/setting_about_us"
            android:textColor="@android:color/white"
            android:textSize="@dimen/px36" />
    </reco.frame.tv.view.TvRelativeLayout>

