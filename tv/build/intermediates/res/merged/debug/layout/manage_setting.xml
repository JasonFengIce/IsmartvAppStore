            android:textColor="@android:color/white"
            android:textSize="@dimen/px36" />

        <TextView
            android:id="@+id/tv_update_hint_flag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/px30"
            android:textColor="@android:color/white"
            android:textSize="@dimen/px36" />
    </reco.frame.tv.view.TvRelativeLayout>


    <reco.frame.tv.view.TvRelativeLayout
        android:id="@+id/trl_upgrade"
        android:layout_width="@dimen/px1112"
        android:layout_height="@dimen/px100"
        android:layout_below="@+id/trl_update_hint"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/px10"
        android:background="@drawable/shape_rectangle_trusculent"
        tv:cursorRes="@drawable/shape_rectangle_cursor"
        tv:boarder="@dimen/px2"
        tv:scalable="false" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
