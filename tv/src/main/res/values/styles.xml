    <style name="activity_anim_scale_alpha">
    
      <item name="@android:activityOpenEnterAnimation">@anim/custom_activity_in</item>

      <item name="@android:activityOpenExitAnimation">@null</item>

      <item name="@android:activityCloseEnterAnimation">@anim/custom_activity_in</item>

      <item name="@android:activityCloseExitAnimation">@anim/custom_activity_out</item>
    
    </style>

